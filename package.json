{
  "name": "arcadia",
  "version": "6.7.0",
  "private": true,
  "scripts": {
    "clean": "rimraf .next out",
    "clean:all": "yarn clean && rimraf node_modules",
    "format": "prettier --write .",
    "check-all": "yarn check-format && yarn lint",
    "check-format": "prettier --check .",
    "lint": "next lint",
    "build": "yarn check-all && next build",
    "start": "yarn dev",
    "dev": "next dev -- -H 0.0.0.0",
    "prod": "next start",
    "export": "next export",
    "rebuild": "yarn clean && yarn build && yarn export"
  },
  "dependencies": {
    "@giancosta86/hermes": "^1.0.4",
    "@giancosta86/hermes-react": "^3.0.0",
    "@giancosta86/time-utils": "^3.0.0",
    "@rimbu/collection-types": "^1.1.0",
    "@rimbu/common": "^1.1.0",
    "@rimbu/list": "^1.1.0",
    "@rimbu/ordered": "^1.1.0",
    "@rimbu/stream": "^1.1.0",
    "classnames": "^2.3.2",
    "next": "^13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18",
    "@types/react": "^18.2.20",
    "eslint": "^8.47.0",
    "eslint-config-next": "^13.1.1",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "sass": "^1.57.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.17.1"
  }
}
