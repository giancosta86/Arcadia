---
layout: null
---
[
{% for post in site.posts %}
    {
        "title": "{{ post.title }}",

        "date": "{{
            post.date
            | date: '%F'
            }}",

        "url": "{{ post.url }}"

        {% if post.version %}
            ,
        "version": {{ post.version }}
        {% endif %}
    }
    {% unless forloop.last %},{% endunless %}
{% endfor %}
]